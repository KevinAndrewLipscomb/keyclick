function KeyclickMailHeaders($db_link)
   {
   $saved_error_level = error_reporting(E_ALL);
   $header_string = RoleEmailAddress($db_link) . "\r\n" //from
      . RoleEmailAddress($db_link) . "\r\n"; //reply_to
   //
   error_reporting($saved_error_level);
   return $header_string;
   }
