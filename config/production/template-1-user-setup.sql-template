create user if not exists 'ki-keyclick'@'127.0.0.1' identified by '~ki-keyclick-password~';
grant select
 , insert
 , update
 , delete
 , create
 , alter
 , drop
  on keyclick.*
  to 'ki-keyclick'@'127.0.0.1'
;
create user if not exists 'ki-keyclickk'@'127.0.0.1' identified by '~ki-keyclickk-password~';
grant select
  on kalipso.*
  to 'ki-keyclickk'@'127.0.0.1'
;
grant select
  on oscardotnetdb.*
  to 'ki-keyclick'@'127.0.0.1'
